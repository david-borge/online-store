{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "web": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/web",
            "sourceRoot": "projects/web/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular/build:application",
                    "options": {
                        "browser": "projects/web/main.ts",
                        "polyfills": ["zone.js"],
                        "tsConfig": "projects/web/tsconfig.app.json",
                        "assets": [
                            "projects/web/src/favicon.ico",
                            "projects/web/src/assets",
                            "projects/web/src/robots.txt",
                            "projects/web/src/sitemap.xml"
                        ],
                        "styles": [
                            "node_modules/bootstrap/scss/bootstrap.scss",
                            "projects/web/src/styles.scss"
                        ],
                        "server": "projects/web/main.server.ts",
                        "outputMode": "server",
                        "ssr": {
                            "entry": "projects/web/src/server.ts"
                        },
                        "inlineStyleLanguage": "scss",
                        "stylePreprocessorOptions": {
                            "includePaths": ["projects/web/src"]
                        }
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "2mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4mb",
                                    "maximumError": "5mb"
                                }
                            ],
                            "outputHashing": "all",
                            "optimization": {
                                "scripts": true,
                                "styles": {
                                    "minify": true,
                                    "inlineCritical": true,
                                    "removeSpecialComments": true
                                },
                                "fonts": {
                                    "inline": true
                                }
                            },
                            "extractLicenses": true,
                            "sourceMap": false,
                            "namedChunks": false,
                            "aot": true,
                            "crossOrigin": "anonymous",
                            "subresourceIntegrity": true,
                            "serviceWorker": false
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular/build:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "web:build:production"
                        },
                        "development": {
                            "buildTarget": "web:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular/build:extract-i18n",
                    "options": {
                        "buildTarget": "web:build"
                    }
                },
                "test": {
                    "builder": "@angular/build:karma",
                    "options": {
                        "polyfills": ["zone.js", "zone.js/testing"],
                        "tsConfig": "projects/web/tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": ["projects/web/src/favicon.ico", "projects/web/src/assets"],
                        "styles": ["projects/web/src/styles.scss"],
                        "scripts": []
                    }
                },
                "deploy": {
                    "builder": "@angular/fire:deploy",
                    "options": {
                        "prerender": false,
                        "ssr": true,
                        "browserTarget": "web:build:production",
                        "firebaseProject": "online-store-7de9d",
                        "firebaseHostingSite": "online-store-7de9d",
                        "serverTarget": "web:server:production",
                        "prerenderTarget": "web:prerender:production"
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false
    },
    "schematics": {
        "@schematics/angular:component": {
            "type": "component"
        },
        "@schematics/angular:directive": {
            "type": "directive"
        },
        "@schematics/angular:service": {
            "type": "service"
        },
        "@schematics/angular:guard": {
            "typeSeparator": "."
        },
        "@schematics/angular:interceptor": {
            "typeSeparator": "."
        },
        "@schematics/angular:module": {
            "typeSeparator": "."
        },
        "@schematics/angular:pipe": {
            "typeSeparator": "."
        },
        "@schematics/angular:resolver": {
            "typeSeparator": "."
        }
    }
}
